        <rdfs:label>roleOf</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/RO_0000081"/>
        <rdfs:seeAlso rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasRole"/>
        <rdfs:seeAlso>ISO 15926-2:2003 &apos;role&apos;</rdfs:seeAlso>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
        <iof-av:usageNote>For annotations, see the inverse relation &apos;has role&apos;.</iof-av:usageNote>
    </owl:ObjectProperty>
    <owl:Axiom>

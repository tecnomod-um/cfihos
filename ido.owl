        <rdfs:label>interestOf</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasInterest"/>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
        <iof-av:usageNote>For annotations, see the inverse relation &apos;has interest&apos;.</iof-av:usageNote>
    </owl:ObjectProperty>
    


    <!-- http://rds.posccaesar.org/ontology/lis14/rdl/isAbout -->


    <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/createdBy">
        <rdfs:subPropertyOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/passiveParticipantIn"/>
